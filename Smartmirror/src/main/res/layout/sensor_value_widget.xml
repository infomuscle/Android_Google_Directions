<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_Popup_Main_Senser"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/transparent"
    >
    <!--android:background="@android:color/transparent"-->
    <TextView
        android:id="@+id/textView_FindDustappwidget_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="1dp"
        android:background="@android:color/transparent"
        android:contentDescription="@string/appwidget_text"
        android:gravity="center_horizontal|center_vertical"
        android:text="미세먼지 : "
        android:textColor="@android:color/white"
        android:textSize="15sp"
        android:visibility="visible" />

    <TextView
        android:id="@+id/textView_Temperatureappwidget_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView_FindDustappwidget_text"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:background="@android:color/transparent"
        android:contentDescription="@string/appwidget_text"
        android:gravity="center_horizontal|center_vertical"
        android:text="온도 : "
        android:textColor="@android:color/white"
        android:textSize="15sp"
        android:visibility="visible" />

    <TextView
        android:id="@+id/textView_Humidityappwidget_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView_Temperatureappwidget_text"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:background="@android:color/transparent"
        android:contentDescription="@string/appwidget_text"
        android:gravity="center_horizontal|center_vertical"
        android:text="습도 : "
        android:textColor="@android:color/white"
        android:textSize="15sp"
        android:visibility="visible" />

</RelativeLayout>